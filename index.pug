doctype
html(lang="es")
   head
      meta(charset="UTF-8")
      meta(name="viewport", content="width=device-width, initial-scale=1.0")
      meta(http-equiv="X-UA-Compatible", content="ie=edge")
      meta(name="application-name", content="BarberShop")
      meta(name="author", content="Gonzalo Fleming")
      meta(name="description", content="Blog de barbería BarberShop")
      meta(name="keywords", content="barbería, barba")
      link(rel="shortcut icon", href="assets/img/favicon/favicon.ico", type="image/x-icon")
      script(src="https://kit.fontawesome.com/ac845de1ae.js", crossorigin="anonymous")
      link(rel="stylesheet", href="assets/css/style.css")
      title BarberShop - Blog
	
   body
      nav.navbar.navbar-expand-lg.navbar-light
         .container
            a.navbar-brand(href="#")
               img(src="assets/img/logo.png", alt="Logo Barbería")
            
            button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation")
               span.navbar-toggler-icon
            
            .collapse.navbar-collapse#navbarNav
               ul.navbar-nav.ml-auto
                  li.nav-item: a.nav-link(href="#") Nosotros
                  li.nav-item: a.nav-link(href="#") Trabajos
                  li.nav-item.active: a.nav-link(href="#") Blog
                  li.nav-item: a.nav-link(href="#") Horas
                  li.nav-item: a.nav-link(href="#") Contacto
